      "name": "POST /api/service",
      "event": [
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "pm.test('Status code is 201 (Created)', function () {",
              "    pm.response.to.have.status(201);",
              "})",
              "",
              "pm.test('Response time is under 2000ms', function () {",
              "    pm.expect(pm.response.responseTime).to.be.below(2000);",
              "})",
              "",
              "pm.test('Response has JSON body', function () {",
              "    pm.response.to.be.json;",
              "})",
              "",
              "pm.test('All required fields exist', function () {",
              "    const jsonData = pm.response.json();",
              "    pm.expect(jsonData).to.have.property('id');",
              "    pm.expect(jsonData).to.have.property('name');",
              "    pm.expect(jsonData).to.have.property('quantity');",
              "    pm.expect(jsonData).to.have.property('price');",
              "    pm.expect(jsonData).to.have.property('tax');",
              "    pm.expect(jsonData).to.have.property('gross');",
              "})",
              "",
              "pm.test('Data types are correct', function () {",
              "    const jsonData = pm.response.json();",
              "    pm.expect(jsonData.id).to.be.a('number');",
              "    pm.expect(jsonData.name).to.be.a('string');",
              "    pm.expect(jsonData.quantity).to.be.a('number');",
              "    pm.expect(jsonData.price).to.be.a('number');",
              "    pm.expect(jsonData.tax).to.be.a('number');",
              "    pm.expect(jsonData.gross).to.be.a('number');",
              "})",
              "",
              "pm.test('Created service has correct values from request', function () {",
              "    const jsonData = pm.response.json();",
              "    const requestBody = JSON.parse(pm.request.body.raw);",
              "    pm.expect(jsonData.name).to.eql(requestBody.name);",
              "    pm.expect(jsonData.quantity).to.eql(requestBody.quantity);",
              "    pm.expect(jsonData.price).to.eql(requestBody.price);",
              "    pm.expect(jsonData.gross).to.eql(requestBody.gross);",
              "})",
              "",
              "pm.test('Save created service ID to collection variable', function () {",
              "    const jsonData = pm.response.json();",
              "    pm.collectionVariables.set('serviceId', jsonData.id);",
              "    console.log('Saved serviceId:', jsonData.id);",
              "})"
            ]
          }
        }
